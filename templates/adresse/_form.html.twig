{% form_theme form 'theme/themebcc.html.twig' %}
{{ form_start(form,{
    label: false,
    attr:{'class': 'w-full flex justify-center items-center flex-wrap text-white'}
}) }}
{{ form_row(form.pays) }}
{{ form_row(form.ville) }}
{{ form_row(form.codePostal) }}
{{ form_row(form.rue) }}

<div class="hidden">
    {{ form_row(form._token) }}
</div>
<div class="w-full justify-center items-center m-3">
    <button type="submit"
            class="bg-bcc-green p-3 w-32 justify-center items-center text-center text-xs text-white rounded shadow">
        {{ button_label|default('Save') }}
    </button>
</div>

